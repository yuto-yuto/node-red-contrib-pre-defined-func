    "@types/sinon": "^10.0.11",
    "chai": "^4.3.6",
    "eslint": "^8.15.0",
    "eslint-config-defaults": "^9.0.0",
    "mocha": "^10.0.0",
    "node-red": "^2.2.2",
    "node-red-node-test-helper": "^0.2.7",
    "sinon": "^14.0.0",
